{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"command": "node",
			"args": [
				"${workspaceFolder}\\.vscode\\apply.js",
				"${workspaceFolder}"
			],
			"label": "apply",
			"detail": "生成接口导出表apply.json",
			"problemMatcher": []
		},
		{
			"type": "typescript",
			"tsconfig": "src/tsconfig.json",
			"problemMatcher": [
				"$tsc"
			],
			"label": "tsc",
			"dependsOn": [
				"apply"
			]
		},
		{
			"type": "shell",
			"command": "node",
			"args": [
				"${workspaceFolder}\\.vscode\\require.js",
				"${workspaceFolder}"
			],
			"label": "require",
			"detail": "合成require资源require.js",
			"problemMatcher": [],
			"dependsOn": [
				"tsc"
			]
		},
		{
			"type": "shell",
			"command": "node",
			"args": [
				"${workspaceFolder}\\.vscode\\local.js",
				"${workspaceFolder}"
			],
			"label": "develop",
			"detail": "编译开发版脚本local.user.js",
			"problemMatcher": [],
			"dependsOn": [
				"require"
			]
		},
		{
			"type": "shell",
			"command": "node",
			"args": [
				"${workspaceFolder}\\.vscode\\main.js",
				"${workspaceFolder}"
			],
			"label": "release",
			"detail": "编译发行版脚本main.user.js",
			"problemMatcher": [],
			"dependsOn": [
				"require"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}